import socket

HOST = "26.84.232.20"  # Nome de host ou endere√ßo IP do servidor
PORT = 9999 # A porta usada pelo servidor

print("conectando")
with socket.socket(socket.AF_INET, socket.SOCK_STREAM) as s:
    print("conectando2")
    s.connect((HOST, PORT))
    print("conectado")
    s.sendall(b"Hello, world")
    data = s.recv(1024)

print("Recebido", repr(data))

